@article{Brown:2008bf,
author = {Brown, M C and Levine, J L},
title = {{Dendrites of medial olivocochlear neurons in mouse.}},
journal = {Neuroscience},
year = {2008},
volume = {154},
number = {1},
pages = {147--159},
month = jun,
affiliation = {Department of Otology and Laryngology, Harvard Medical School, Boston, MA 02115, USA. Chris_Brown@meei.harvard.edu},
doi = {10.1016/j.neuroscience.2007.12.045},
pmid = {18313859},
pmcid = {PMC2587447},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-03-23T22:58:09GMT},
date-modified = {2016-02-15T21:30:27GMT},
abstract = {Stains for acetylcholinesterase (AChE) and retrograde labeling with Fluorogold (FG) were used to study olivocochlear neurons and their dendritic patterns in mice. The two methods gave similar results for location and number of somata. The total number of medial olivocochlear (MOC) neurons in the ventral nucleus of the trapezoid body (VNTB) is about 170 per side. An additional dozen large olivocochlear neurons are located in the dorsal periolivary nucleus (DPO). Dendrites of all of these neurons are long and extend in all directions from the cell bodies, a pattern that contrasts with the sharp frequency tuning of their responses. For VNTB neurons, there were greater numbers of dendrites directed medially than laterally and those directed medially were longer (on average, 25-50% longer). Dendrite extensions were most pronounced for neurons located in the rostral portion of the VNTB. When each dendrite from a single neuron was represented as a vector, and all the vectors summed, the result was also skewed toward the medial direction. DPO neurons, however, had more symmetric dendrites that projected into more dorsal parts of the trapezoid body, suggesting that this small group of olivocochlear neurons has very different physiological properties. Dendrites of both types of neurons were somewhat elongated rostrally, about 20% longer than those directed caudally. These results can be interpreted as extensions of dendrites of olivocochlear neurons toward their synaptic inputs: medially to meet crossing fibers from the cochlear nucleus that are part of the MOC reflex pathway, and rostrally to meet descending inputs from higher centers.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=18313859&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/C0/C0E7BBC6-517E-42F6-9D42-DBC1ED7D425D.pdf},
file = {{C0E7BBC6-517E-42F6-9D42-DBC1ED7D425D.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/C0/C0E7BBC6-517E-42F6-9D42-DBC1ED7D425D.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.neuroscience.2007.12.045}}
}


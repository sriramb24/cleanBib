@article{Kim:2009dk,
author = {Kim, Jeesun and Davis, Chris and Groot, Christopher},
title = {{Speech identification in noise: Contribution of temporal, spectral, and visual speech cues.}},
journal = {The Journal of the Acoustical Society of America},
year = {2009},
volume = {126},
number = {6},
pages = {3246--3257},
month = dec,
affiliation = {MARCS Auditory Laboratories, University of Western Sydney, NSW 1797, Australia.},
doi = {10.1121/1.3250425},
pmid = {20000938},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-09-27T18:39:46GMT},
date-modified = {2016-02-15T21:30:25GMT},
abstract = {This study investigated the degree to which two types of reduced auditory signals (cochlear implant simulations) and visual speech cues combined for speech identification. The auditory speech stimuli were filtered to have only amplitude envelope cues or both amplitude envelope and spectral cues and were presented with/without visual speech. In Experiment 1, IEEE sentences were presented in quiet and noise. For in-quiet presentation, speech identification was enhanced by the addition of both spectral and visual speech cues. Due to a ceiling effect, the degree to which these effects combined could not be determined. In noise, these facilitation effects were more marked and were additive. Experiment 2 examined consonant and vowel identification in the context of CVC or VCV syllables presented in noise. For consonants, both spectral and visual speech cues facilitated identification and these effects were additive. For vowels, the effect of combined cues was underadditive, with the effect of spectral cues reduced when presented with visual speech cues. Analysis indicated that without visual speech, spectral cues facilitated the transmission of place information and vowel height, whereas with visual speech, they facilitated lip rounding, with little impact on the transmission of place information.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20000938&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A6/A6741089-E4E1-40C7-94C3-CF999553645C.pdf},

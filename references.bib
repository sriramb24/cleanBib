@article{Feliciano:1995wu,
author = {Feliciano, M and Potashner, S J},
title = {{Evidence for a glutamatergic pathway from the guinea pig auditory cortex to the inferior colliculus.}},
journal = {Journal of Neurochemistry},
year = {1995},
volume = {65},
number = {3},
pages = {1348--1357},
affiliation = {Laboratory of Neuromorphology, University of Connecticut, Storrs, USA.},
pmid = {7643112},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-05-06T05:04:07GMT},
date-modified = {2016-02-15T21:30:29GMT},
abstract = {We attempt to provide evidence that the projection from the guinea pig auditory cortex (AC) to the inferior colliculus (IC) may contain glutamatergic or GABAergic fibers. Seven days after unilateral AC aspiration, histological studies indicated almost complete AC destruction and preterminal degeneration of fibers and terminal fields in the dorsal cortex (DCIC), external cortex (ECIC), and central nucleus (CNIC) of the IC ipsilateral to the ablated AC. Contralaterally, degeneration appeared in the DCIC. AC ablation depressed the electrically evoked Ca(2+)-dependent release of D-[3H]aspartate (D-[3H]Asp) in the ipsilateral DCIC, ECIC, and CNIC, and D-[3H]Asp uptake in the CNIC. Together with other evidence that the corticocollicular pathway is excitatory, these findings suggest that this projection may contain glutamatergic and/or aspartatergic (Glu/Asp-ergic) fibers. Glutamic acid decarboxylase immunoreactivity was not apparent in presumed pyramidal cells of layer V of the AC retrogradely labeled with biotinylated dextran injected into the ipsilateral IC. Thus, corticocollicular neurons probably do not synthesize GABA and may not be GABAergic. However, AC ablation depressed [14C]GABA release from the ipsilateral DCIC and ECIC, and [14C]GABA uptake in the DCIC. These findings are consistent with the atrophy or down-regulation of some subcortical neurons that mediate GABAergic transmission in the IC.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=7643112&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/22/227117E3-9E22-4BA1-9814-9E6199851635.pdf},
file = {{227117E3-9E22-4BA1-9814-9E6199851635.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/22/227117E3-9E22-4BA1-9814-9E6199851635.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/B6BBFD0B-54D9-4361-9A18-4D9D19BBA7C7}}
}


@article{Dawes:2009ga,
author = {Dawes, Piers and Bishop, Dorothy},
title = {{Auditory processing disorder in relation to developmental disorders of language, communication and attention: a review and critique.}},
journal = {International Journal of Language {\&} Communication Disorders},
year = {2009},
volume = {44},
number = {4},
pages = {440--465},
affiliation = {Department of Experimental Psychology, University of Oxford, Oxford, UK. piers.dawes@manchester.ac.uk},

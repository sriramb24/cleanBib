@article{Lovett:2012if,
author = {Lovett, Rosemary Elizabeth Susan and Kitterick, Padraig Thomas and Huang, Shan and Summerfield, Arthur Quentin},
title = {{The Developmental Trajectory of Spatial Listening Skills in Normal-Hearing Children}},
journal = {Journal of Speech, Language, and Hearing Research},
year = {2012},
volume = {55},
number = {3},
pages = {865--878},
annote = {ly one systematic revieqw },
affiliation = {University of York, Heslington, York, United Kingdom. r.lovett@ucl.ac.uk},
doi = {10.1044/1092-4388(2011/11-0096)},
pmid = {22271871},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-06-08T00:06:19GMT},
date-modified = {2016-02-15T21:30:30GMT},
abstract = {PURPOSE:To establish the age at which children can complete tests of spatial listening and to measure the normative relationship between age and performance.

METHOD:Fifty-six normal-hearing children, ages 1.5-7.9 years, attempted tests of the ability to discriminate a sound source on the left from one on the right, to localize a source, to track moving sources, and to perceive speech in noise.

RESULTS:Tests of left-right discrimination, movement tracking, and speech perception were completed by $\ge$ 75% of children older than 3 years. Children showed adult levels of performance from age 1.5 years (movement tracking), 3 years (left-right discrimination), and 6 years (localization and speech in noise). Spatial release from masking-calculated as the difference in speech reception thresholds between conditions with spatially coincident and spatially separate sp-eech and noise--remained constant at 5 dB from age 3 years. Data from a separate study demonstrate the age at which children with cochlear implants can complete the same tests. Assessments of left-right discrimination, movement tracking, and speech perception were completed by $\ge$ 75% of children who are older than 5 years and who wear cochlear implants.

CONCLUSION:These data can guide the selection of tests for future studies and inform the interpretation of results from clinical populations.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22271871&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/B5/B5184507-E25D-412E-B4A0-042EDA13988C.pdf},

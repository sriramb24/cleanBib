@article{Brown:2010eia,
author = {Brown, Guy J and Ferry, Robert T and Meddis, Ray},
title = {{A computer model of auditory efferent suppression: implications for the recognition of speech in noise.}},
journal = {The Journal of the Acoustical Society of America},
year = {2010},
volume = {127},
number = {2},
pages = {943--954},
month = feb,
affiliation = {Department of Computer Science, University of Sheffield, Sheffield S1 4DP, United Kingdom. g.brown@dcs.shef.ac.uk},
doi = {10.1121/1.3273893},
pmid = {20136217},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-02-10T03:45:23GMT},
date-modified = {2016-02-15T21:30:34GMT},
abstract = {The neural mechanisms underlying the ability of human listeners to recognize speech in the presence of background noise are still imperfectly understood. However, there is mounting evidence that the medial olivocochlear system plays an important role, via efferents that exert a suppressive effect on the response of the basilar membrane. The current paper presents a computer modeling study that investigates the possible role of this activity on speech intelligibility in noise. A model of auditory efferent processing [Ferry, R. T., and Meddis, R. (2007). J. Acoust. Soc. Am. 122, 3519-3526] is used to provide acoustic features for a statistical automatic speech recognition system, thus allowing the effects of efferent activity on speech intelligibility to be quantified. Performance of the "basic" model (without efferent activity) on a connected digit recognition task is good when the speech is uncorrupted by noise but falls when noise is present. However, recognition performance is much improved when efferent activity is applied. Furthermore, optimal performance is obtained when the amount of efferent activity is proportional to the noise level. The results obtained are consistent with the suggestion that efferent suppression causes a "release from adaptation" in the auditory-nerve response to noisy speech, which enhances its intelligibility.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20136217&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A3/A370DC0D-357E-4AEC-8300-7B5811748711.pdf},
file = {{A370DC0D-357E-4AEC-8300-7B5811748711.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A3/A370DC0D-357E-4AEC-8300-7B5811748711.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1121/1.3273893}}
}


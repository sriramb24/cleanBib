@article{Kawase:2000tx,
author = {Kawase, T and Ogura, M and Hidaka, H and Sasaki, N and Suzuki, Y and Takasaka, T},
title = {{Effects of contralateral noise on measurement of the psychophysical tuning curve.}},
journal = {Hearing Research},
year = {2000},
volume = {142},
number = {1-2},
pages = {63--70},
month = apr,
affiliation = {Department of Otolaryngology, Head and Neck Surgery, Tohoku University Graduate School of Medicine, 1-1 Seiryo-machi, Aoba-ku, Sendai, Japan. kaw@mail.cc.tohoku.ac.jp},
pmid = {10748329},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-01-07T18:09:45GMT},
date-modified = {2016-02-15T21:30:28GMT},
abstract = {The effects of the addition of contralateral noise on the psychophysical tuning curve (PTC) were examined in subjects with normal hearing. The masking threshold of the tail part of the PTC tended to decrease with the addition of contralateral noise, although the threshold reduction was usually less than 5 dB. On the other hand, the effects of contralateral noise were relatively small around the tip of the PTC contour. Focusing on the effects of contralateral noise on the masking threshold at the tail part of the PTC, the effects of changing the time between initiation of masking the tone and the presentation of the masked probe tone on the threshold reduction at the tail part of the PTC were also observed. The results indicate that the reduction of the masking threshold by the addition of contralateral noise tended to be larger when the presentation of the signal tone was delayed after the onset of the masker. Usually, when the signal tone was presented under conditions of the forward masking paradigm, the reduction of the threshold was most remarkable. Results obtained in the present study are discussed based on the known characteristics of the olivocochlear (OC)-efferent fibers activated by contralateral noise.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10748329&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/90/90CE47C7-A53C-444F-A82D-06A34525613C.pdf},

@article{Colliver:2000uy,
author = {Colliver, J A},
title = {{Effectiveness of problem-based learning curricula: research and theory.}},
journal = {Academic medicine},
year = {2000},
volume = {75},
number = {3},
pages = {259--266},
month = mar,
affiliation = {Statistics and Research Consulting, Southern Illinois University School of Medicine, Springfield 62794-1217, USA. colliver@siumed.edu},
pmid = {10724315},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-06-28T01:19:32GMT},
date-modified = {2016-02-15T21:30:27GMT},
abstract = {PURPOSE:This article provides a critical overview of problem-based learning (PBL), its effectiveness for knowledge acquisition and clinical performance, and the underlying educational theory. The focus of the paper is on (1) the credibility of claims (both empirical and theoretical) about the ties between PBL and educational outcomes and (2) the magnitude of the effects.

METHOD:The author reviewed the medical education literature, starting with three reviews published in 1993 and moving on to research published from 1992 through 1998 in the primary sources for research in medical education. For each study the author wrote a summary, which included study design, outcome measures, effect sizes, and any other information relevant to the research conclusion.

RESULTS AND CONCLUSION:The review of the literature revealed no convincing evidence that PBL improves knowledge base and clinical performance, at least not of the magnitude that would be expected given the resources required for a PBL curriculum. The results were considered in light of the educational theory that underlies PBL and its basic research. The author concludes that the ties between educational theory and research (both basic and applied) are loose at best.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10724315&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/B4/B4211ACB-0527-40CA-A95C-7DA8FD594C67.pdf},

@article{Whitmer:2012ht,
author = {Whitmer, William M and Seeber, Bernhard U and Akeroyd, Michael A},
title = {{Apparent auditory source width insensitivity in older hearing-impaired individuals.}},
journal = {The Journal of the Acoustical Society of America},
year = {2012},
volume = {132},
number = {1},
pages = {369--379},
month = jul,
affiliation = {MRC Institute of Hearing Research, Scottish Section, Glasgow Royal Infirmary, Glasgow G31 2ER, United Kingdom. bill@ihr.gla.ac.uk},
doi = {10.1121/1.4728200},
pmid = {22779484},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-09-26T04:08:12GMT},
date-modified = {2016-02-15T21:30:25GMT},
abstract = {Previous studies have shown a loss in the precision of horizontal localization responses of older hearing-impaired (HI) individuals, along with potentially poorer neural representations of sound-source location. These deficits could be the result or corollary of greater difficulties in discriminating spatial images, and the insensitivity to punctate sound sources. This hypothesis was tested in three headphone-presentation experiments varying interaural coherence (IC), the cue most associated with apparent auditory source width. First, thresholds for differences in IC were measured for a broad sampling of participants. Older HI participants were significantly worse at discriminating IC across reference values than younger normal-hearing participants. These results are consistent with senescent increases in temporal jitter. Performance decreased with age, a finding corroborated in a second discrimination experiment using a separate group of participants matched for hearing loss. This group also completed a third, visual experiment, with both a cross-mapping task where they drew the size of the sound they heard and the identification task where they chose the image that best corresponded to what they heard. The results from the visual tasks indicate that older HI individuals do not hear punctate images and are relatively insensitive to changes in width based on IC.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22779484&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A7/A7E89594-2519-48E7-A508-82DCAF91C30A.pdf},
file = {{A7E89594-2519-48E7-A508-82DCAF91C30A.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A7/A7E89594-2519-48E7-A508-82DCAF91C30A.pdf:application/pdf}},

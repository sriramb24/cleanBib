@article{Hartley:2000ug,
author = {Hartley, D E and Wright, B A and Hogan, S C and Moore, David R},
title = {{Age-related improvements in auditory backward and simultaneous masking in 6- to 10-year-old children.}},
journal = {Journal of Speech, Language, and Hearing Research},
year = {2000},
volume = {43},
number = {6},
pages = {1402--1415},
affiliation = {University Laboratory of Physiology, Oxford, England, UK. douglas.hartley@physiol.ox.ac.uk},

title = {{Cortical tonotopic map plasticity and behavior.}},
journal = {Neuroscience and Biobehavioral Reviews},
year = {2011},
volume = {35},
number = {10},
pages = {2117--2128},
month = nov,
affiliation = {Hotchkiss Brain Institute, Departments of Physiology and Pharmacology, University of Calgary, Calgary, Alberta, Canada.},
doi = {10.1016/j.neubiorev.2011.02.002},

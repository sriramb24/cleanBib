@article{Maison:1999tf,
author = {Maison, S F and Micheyl, C and Collet, L},
title = {{Sinusoidal amplitude modulation alters contralateral noise suppression of evoked otoacoustic emissions in humans.}},
journal = {Neuroscience},
year = {1999},
volume = {91},
number = {1},
pages = {133--138},
affiliation = {Universit{\'e} Claude Bernard Lyon 1, Laboratoire Neurosciences and Syst{\`e}mes Sensoriels, UPRESA CNRS 5020, Lyon, France.},
pmid = {10336065},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-01-12T22:53:27GMT},
date-modified = {2016-02-15T21:30:21GMT},
abstract = {It is well established that low-level broad band noise can elicit an amplitude decrease in evoked otoacoustic emissions recorded in the opposite ear. However, the influence of the temporal characteristics of the contralateral stimulus on this effect remains largely unknown. In the present study, otoacoustic emissions evoked by 60 dB SPL clicks were recorded in 19 normal-hearing subjects using the Otodynamics IL088, successively in absence and presence of a contralateral noise that was either steady or modulated sinusoidally in amplitude at different depths (from 25% to 100% in 25 point steps) and rates (from 50 Hz to 800 Hz in half-octave steps). The energy was kept constant whatever the modulation depth. The results showed that the evoked otoacoustic-emission attenuation effect induced by contralateral stimulation varied depending on the modulation depth and frequency of the contralateral amplitude-modulated noise. The largest suppression effect was observed at the 100 Hz modulation frequency and the 100% modulation depth. The 50 Hz modulation resulted in less suppression than with unmodulated noise. An interpretation of these results in terms of the influence of temporal amplitude fluctuations falling within a certain range on medial olivocochlear bundle activity is discussed.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10336065&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/AE/AEA679AE-EA2B-457A-A65F-919760C3E5C0.pdf},
file = {{AEA679AE-EA2B-457A-A65F-919760C3E5C0.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/AE/AEA679AE-EA2B-457A-A65F-919760C3E5C0.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/F4FD7514-2CE2-4EF4-836A-B1816BD0F846}}
}

@article{Ahissar:2004fc,
author = {Ahissar, Merav and Hochstein, Shaul},
title = {{The reverse hierarchy theory of visual perceptual learning.}},
journal = {Trends in Cognitive Sciences},
year = {2004},
volume = {8},
number = {10},
pages = {457--464},
affiliation = {Department of Psychology and Interdisciplinary Center for Neural Computation, Hebrew University, Jerusalem 91905, Israel. msmerava@pluto.huji.ac.il},
doi = {10.1016/j.tics.2004.08.011},
pmid = {15450510},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-09-14T21:42:06GMT},
date-modified = {2016-02-15T21:30:33GMT},
abstract = {Perceptual learning can be defined as practice-induced improvement in the ability to perform specific perceptual tasks. We previously proposed the Reverse Hierarchy Theory as a unifying concept that links behavioral findings of visual learning with physiological and anatomical data. Essentially, it asserts that learning is a top-down guided process, which begins at high-level areas of the visual system, and when these do not suffice, progresses backwards to the input levels, which have a better signal-to-noise ratio. This simple concept has proved powerful in explaining a broad range of findings, including seemingly contradicting data. We now extend this concept to describe the dynamics of skill acquisition and interpret recent behavioral and electrophysiological findings.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=15450510&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/58/58C62DA6-E9E1-4295-950D-56A2ECF85693.pdf},

@article{Frisina:1990cw,
author = {Frisina, R D and Smith, R L and Chamberlain, S C},
title = {{Encoding of amplitude modulation in the gerbil cochlear nucleus: I. A hierarchy of enhancement.}},
journal = {Hearing Research},
year = {1990},
volume = {44},
number = {2-3},
pages = {99--122},
month = mar,
affiliation = {Otolaryngology Division of Surgery Department, University of Rochester School of Medicine, New York 14642.},
doi = {10.1016/0378-5955(90)90074-Y},
pmid = {2329098},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-09-12T19:44:02GMT},
date-modified = {2016-02-15T21:30:26GMT},
abstract = {The main goal of the present study was to investigate the encoding of a biologically-relevant acoustic feature--amplitude modulation (AM)--in single neurons of the auditory nerve and ventral cochlear nucleus (VCN). In the anesthetized gerbil auditory-nerve fibers and VCN units show strong synchronous responses to low-intensity, low-frequency AM. As frequency increases, the strength of the synchronous response decreases. In the auditory nerve the strength of the synchronous response is substantially less at high intensities than at low intensities and does not change significantly with AM frequency at high intensities. In contrast to the auditory nerve, VCN units show strong responses at high intensities. They have a particular AM frequency to which they are maximally responsive, and this frequency varies from unit to unit. Therefore, VCN units transform their ascending inputs by enhancing the synchronous response to AM. A correlation exists between a unit's ability to encode AM and its responses to simple sounds. Specifically, onset units show the strongest synchronous responses, followed in order by chopper, primarylike-with-notch and primarylike units. This enhancement is greatest at high intensities and can occur up to 90 dB above a unit's threshold. Thus, a hierarchy of enhancement for AM processing exists in the most peripheral nucleus of the central auditory system.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=2329098&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/AF/AFBE9422-238D-454B-884F-D7085BE7781A.pdf},
file = {{AFBE9422-238D-454B-884F-D7085BE7781A.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/AF/AFBE9422-238D-454B-884F-D7085BE7781A.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/0378-5955(90)90074-Y}}
}


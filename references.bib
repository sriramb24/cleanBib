@article{King:2001tr,
author = {King, A J and Schnupp, J W H and Doubell, T P},
title = {{The shape of ears to come: dynamic coding of auditory space.}},
journal = {Trends in Cognitive Sciences},
year = {2001},
volume = {5},
number = {6},
pages = {261--270},
month = jun,
affiliation = {University Laboratory of Physiology, Parks Road, OX1 3PT, Oxford, UK},
pmid = {11390297},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-09-17T15:22:29GMT},
date-modified = {2016-02-15T21:30:25GMT},
abstract = {In order to pinpoint the location of a sound source, we make use of a variety of spatial cues that arise from the direction-dependent manner in which sounds interact with the head, torso and external ears. Accurate sound localization relies on the neural discrimination of tiny differences in the values of these cues and requires that the brain circuits involved be calibrated to the cues experienced by each individual. There is growing evidence that the capacity for recalibrating auditory localization continues well into adult life. Many details of how the brain represents auditory space and of how those representations are shaped by learning and experience remain elusive. However, it is becoming increasingly clear that the task of processing auditory spatial information is distributed over different regions of the brain, some working hierarchically, others independently and in parallel, and each apparently using different strategies for encoding sound source location.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=11390297&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/73/73827E01-D32D-4525-B939-26BE03A11130.pdf},
file = {{73827E01-D32D-4525-B939-26BE03A11130.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/73/73827E01-D32D-4525-B939-26BE03A11130.pdf:application/pdf;73827E01-D32D-4525-B939-26BE03A11130.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/73/73827E01-D32D-4525-B939-26BE03A11130.pdf:application/pdf}},

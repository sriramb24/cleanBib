@article{Dalhoff:2012hl,
author = {Dalhoff, Ernst and Turcanu, Diana and Vete{\v s}n{\'\i}k, Ale{\v s} and Gummer, Anthony W},
title = {{Two-source interference as the major reason for auditory-threshold estimation error based on DPOAE input-output functions in normal-hearing subjects.}},
journal = {Hearing Research},
year = {2012},
month = dec,
affiliation = {Eberhard-Karls-University T{\"u}bingen, Department of Otolaryngology, Section of Physiological Acoustics and Communication, Elfriede-Aulhorn-Stra{\ss}e 5, 72076 T{\"u}bingen, Germany. Electronic address: ernst.dalhoff@uni-tuebingen.de.},
doi = {10.1016/j.heares.2012.12.003},
pmid = {23268357},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-01-09T22:15:24GMT},
date-modified = {2016-02-15T21:30:27GMT},
abstract = {Fine structure in the frequency response of distortion product otoacoustic emissions (DPOAEs) can severely limit the usefulness of DPOAEs in estimating auditory thresholds. Here, fine structure is removed by extracting the primary-source DPOAE component using the onset-decomposition technique (Vete{\v s}n{\'\i}k et~al., 2009) and auditory threshold estimates are compared to those obtained from DPOAEs in response to conventional, continuous two-tone stimulation. Auditory thresholds are predicted using the estimated distortion product thresholds (EDPTs), obtained from linear regression of input-output (I/O) functions of DPOAE pressure amplitude versus second-tone stimulus level (Boege and Janssen, 2002). The accuracy of the auditory-threshold predictions is derived by comparison with measured auditory thresholds. The parameters of the two primary stimulus tones of frequency f(1) and f(2) and levels of L(1) and L(2) are chosen as: f(2)/f(1)~=~1.2 with 1.5~$\le$~f(2)~$\le$~2.5~kHz, and L(1)~=~0.4L(2)~+~39~dB SPL, with 25~$\le$~L(2)~$\le$~65~dB SPL. Data are from 12 normal-hearing subjects with profound DPOAE fine structure. 255 DPOAE I/O functions were measured for each of the two DPOAE paradigms. An EDPT value was accepted as reliable if: 1) the squared correlation coefficient, r(2)~$\ge$~0.8, 2) the regression slope, s(I/O)~$\ge$~0.2~$\mu$Pa/dB, and 3) the standard deviation of the EDPT, $\sigma$(EDPT)~$\le$~10~dB. The proportion of rejected I/O functions was 8% for onset-decomposition DPOAEs, and 25% for continuous-tone DPOAEs. Removal of data points from the saturation region of the DPOAE I/O function by an automated algorithm reduced the rejection rate, to zero for onset-decomposition DPOAEs, but to only 13% for continuous-tone DPOAEs. In the absence of saturated DPOAE responses, auditory thresholds were predicted with standard deviation of only 4~dB for onset-decomposition DPOAEs, but 12~dB for continuous-tone DPOAEs. In summary, by extracting the primary-source component of the DPOAE by the method of onset-decomposition it is possible to predict human auditory threshold with hitherto unattainable accuracy.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23268357&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A0/A0012F77-301F-41CF-A54F-880BEE07250D.pdf},
file = {{A0012F77-301F-41CF-A54F-880BEE07250D.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A0/A0012F77-301F-41CF-A54F-880BEE07250D.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.heares.2012.12.003}}

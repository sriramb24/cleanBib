@article{vandenBerge:1990tc,
author = {van den Berge, H and Kingma, H and Kluge, C and Marres, E H},
title = {{Electrophysiological aspects of the middle ear muscle reflex in the rat: latency, rise time and effect on sound transmission.}},
journal = {Hearing Research},
year = {1990},
volume = {48},
number = {3},
pages = {209--219},

language = {English},
read = {Yes},
rating = {0},
date-added = {2012-10-19T05:05:14GMT},
date-modified = {2016-02-15T21:30:25GMT},
abstract = {We hypothesized that the Digit Span (DS) subtest and component tasks (Wechsler, 1991) would show strong relationships with a dichotic listening test (Musiek, 1983). In two sets of archival clinical data (N = 74 and N = 51) we demonstrated that: (a) individuals with central auditory deficits had lower DS scores, F(1, 72) = 7.34, p = .008; eta2 = .09; and (b) left-ear dichotic deficits impacted forward span, F(2, 48) = 8.45, p = .001. Right-ear dichotic listening performance also accounted for significant variance in digit forward span (R2 = 0.17, p = .003). While limited in scope, the studies conclude that forward but not reverse span performance is strongly related to dichotic listening, and can serve as a marker for possible central auditory deficits.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=15621853&retmode=ref&cmd=prlinks},

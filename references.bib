@article{Maison:2001uj,
author = {Maison, S F and Micheyl, C and Collet, L},
title = {{Influence of focused auditory attention on cochlear activity in humans.}},
journal = {Psychophysiology},
year = {2001},
volume = {38},
number = {1},
pages = {35--40},
affiliation = {Universit{\'e} Claude Bernard, Laboratoire Neurosciences et Syst{\`e}mes Sensoriels, H pital E. Herriot, Lyon, France. smaison@olfac.univ-lyonl.fr},
pmid = {11321619},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-12-07T02:18:10GMT},
date-modified = {2016-02-15T21:30:27GMT},
abstract = {The mammalian auditory system contains descending pathways that originate in the cortex and relay at various intermediate levels before reaching the peripheral sensory organ of Corti. The last link in this chain consists of the olivocochlear bundle. The activity of this bundle can be measured through otoacoustic emissions, which are acoustic signatures of the cochlear biomechanical activity. In the present study, it was hypothesized that frequency-specific activation of the olivocochlear bundle in the contralateral ear would show up as frequency-specific variations in otoacoustic emission amplitude in the ipsilateral ear. Two groups of young adult subjects participated in this experiment. Evoked otoacoustic emissions were recorded in the ipsilateral ear at two test frequencies (1 and 2 kHz). Subjects had to detect probe tones at a given frequency in background noise in the contralateral ear. Larger efferent activation was measured at test frequencies on which attention is focused. This result provides evidence for an influence of attention on the auditory periphery via descending projections.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=11321619&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/E9/E9ED4D36-C6EF-4705-A45C-3B4F2D92D00E.pdf},
file = {{E9ED4D36-C6EF-4705-A45C-3B4F2D92D00E.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/E9/E9ED4D36-C6EF-4705-A45C-3B4F2D92D00E.pdf:application/pdf}},

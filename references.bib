@article{Long:2008cu,
author = {Long, Glenis R and Talmadge, Carrick L and Lee, Jungmee},
title = {{Measuring distortion product otoacoustic emissions using continuously sweeping primariesa)}},
journal = {The Journal of the Acoustical Society of America},
year = {2008},
volume = {124},
number = {3},

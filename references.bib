pages = {2178--2189},
month = oct,
affiliation = {MRC Institute of Hearing Research, Scottish Section, Glasgow Royal Infirmary-Queen Elizabeth Building, 16 Alexandra Parade, Glasgow G31 2ER, Scotland. warddrennan@yahoo.com},

year = {2015},
volume = {137},
number = {2},
pages = {768--776},
month = feb,
affiliation = {Dipartimento Igiene del Lavoro, INAIL (Italian Workers Compensation Authority) Research, Via Fontana Candida 1, 00040 Monte Porzio Catone, Rome, Italy.},

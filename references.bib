@article{Terreros:2015jf,
author = {Terreros, Gonzalo and Delano, Paul H},
title = {{Corticofugal modulation of peripheral auditory responses.}},
journal = {Frontiers in Systems Neuroscience},
year = {2015},
volume = {9},
pages = {134},
affiliation = {Programa de Fisiolog{\'\i}a y Biof{\'\i}sica, ICBM, Facultad de Medicina, Universidad de Chile Santiago, Chile.},
doi = {10.3389/fnsys.2015.00134},

@article{Siegel:1982ur,
author = {Siegel, J H and Kim, D O},
title = {{Efferent neural control of cochlear mechanics? Olivocochlear bundle stimulation affects cochlear biomechanical nonlinearity.}},
journal = {Hearing Research},
year = {1982},
volume = {6},
number = {2},
pages = {171--182},
month = feb,
pmid = {7061350},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-07-25T16:55:57GMT},
date-modified = {2016-02-15T21:30:30GMT},
abstract = {We confirm the report of Mountain (Mountain, D.C. (1980): Science 210, 77-72) that stimulating the crossed olivocochlear bundle (COCB) can change the magnitude of the distortion product (f2-f1) in the ear-canal sound pressure. Our results are extended to include (2f1-f2) as well as (f2-f1) from anesthetized chinchillas with both middle-ear muscles sectioned. In contrast to Mountain's report, the polarity of the change can be either positive, negative or absent, depending on the choice of two-tone stimulus frequencies. The influence of two-tone stimulus level is also complex, but we have not yet seen the polarity of the COCB effect change with stimulus level. The magnitude and polarity of the change in (2f1-f2) are not simply related to those for (f2-f1). The effect of COCB stimulation is abolished when scala tympani is perfused with artificial perilymph containing 10(-5) M d-tubocurarine. These results demonstrate that the COCB effect is postsynaptic, probably mediated by outer hair cells. We suggest that the normal cochlea contains an active biomechanical mechanism which reduces the damping of the cochlear-partition motion and is modulated by activating the efferents. It is thus possible that the central nervous system may be able to control the dynamics of the motion of the cochlear partition.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=7061350&retmode=ref&cmd=prlinks},

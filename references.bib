@article{Litovsky:1997tc,
author = {Litovsky, Ruth Y},
title = {{Developmental changes in the precedence effect: estimates of minimum audible angle.}},
journal = {The Journal of the Acoustical Society of America},
year = {1997},
volume = {102},
number = {3},
pages = {1739--1745},
affiliation = {Psychology Department, University of Massachusetts, Amherst 01003, USA.},
pmid = {9301051},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-04-28T17:38:18GMT},
date-modified = {2016-02-15T21:30:29GMT},
abstract = {The precedence effect refers to an auditory phenomenon which occurs when two similar sounds are presented from different locations with a brief delay, and only one sound is heard whose perceived location is dominated by the first source. Although the lagging source si not localized as an independent event, under some conditions, adults are able to extract its directional cues. Developmental studies suggest that this ability changes during the development. However, those studies have used stimulus configurations which minimize the measurement of that ability. In the present study adults were first tested under several conditions, and the one which produced optimal performance was chosen for testing children. Using the minimum audible angle (MAA) task in the azimuthal plane, performance was compared for a single-source condition and two precedence conditions: in lag discrimination the lagging source changed location while the lead remained at midline, and in lead discrimination the reverse occurred. Subjects were 18 months old, 5 years old, and adult. Significant improvements in MAA occurred with an increase in age, especially in the precedence conditions. Within each group, performance was significantly better in single-source condition, followed by the lead and the lag discrimination.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=9301051&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/5D/5DECDB33-2325-4D8F-8D2E-F4746AAFECEE.pdf},
file = {{5DECDB33-2325-4D8F-8D2E-F4746AAFECEE.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/5D/5DECDB33-2325-4D8F-8D2E-F4746AAFECEE.pdf:application/pdf}},

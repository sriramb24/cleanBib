@article{Boothalingam:2014cr,
author = {Boothalingam, Sriram and Purcell, David and Scollie, Susan},
title = {{Influence of 100Hz amplitude modulation on the human medial olivocochlear reflex.}},
journal = {Neuroscience letters},
year = {2014},
volume = {580},
pages = {56--61},
month = sep,
affiliation = {National Centre for Audiology, Western University, London, Ontario, Canada N6G1H1. Electronic address: sboothal@uwo.ca.},
doi = {10.1016/j.neulet.2014.07.048},
pmid = {25102324},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-02-13T21:14:13GMT},
date-modified = {2016-03-07T17:32:16GMT},
abstract = {It is well known that medial olivocochlear system (MOC) activity causes inhibition of cochlear amplification that can be measured using otoacoustic emissions (OAEs). The temporal characteristics of this MOC inhibitory effect are still not well understood. Two experiments were performed to further explore a previously reported enhancement in MOC inhibition of OAEs by a broadband noise (BBN) elicitor modulated at 100Hz (AM-BBN). In experiment I, MOC inhibition was measured for toneburst (1 and 2kHz presented at 41.67Hz) and stimulus-frequency (0.96-1.92kHz) OAEs for two elicitor conditions, BBN and AM-BBN (100% modulation depth [MD]), in 27 young normal hearing adults. In experiment II, tonebursts were presented at 50Hz instead of 41.67Hz to test if the previously reported enhancement of the MOC response to 100Hz AM-BBN is specific to a 50Hz toneburst presentation rate. All elicitors caused significant reduction of both TB- and SF-OAE amplitude. AM-BBN evoked the same OAE inhibition compared to BBN in both experiments. This pattern was consistent across OAE types, and toneburst presentation rates. Results suggest that the MOC is not especially sensitive to 100Hz AM-BBN; instead, AM dips in noise energy likely reduce its effectiveness in evoking MOC activity due to temporal energy integration.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=25102324&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/DE/DE428337-C4D3-40E9-9D7C-ECA567658574.pdf},
file = {{DE428337-C4D3-40E9-9D7C-ECA567658574.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/DE/DE428337-C4D3-40E9-9D7C-ECA567658574.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.neulet.2014.07.048}}
}


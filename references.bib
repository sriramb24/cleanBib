@article{Abdala:2001iea,
author = {Abdala, Carolina},
title = {{Maturation of the human cochlear amplifier: Distortion product otoacoustic emission suppression tuning curves recorded at low and high primary tone levels}},
journal = {The Journal of the Acoustical Society of America},
year = {2001},
volume = {110},
number = {3},
pages = {1465--1476},
publisher = {Acoustical Society of America},
doi = {10.1121/1.1388018},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-01-21T02:39:48GMT},
date-modified = {2016-02-15T21:30:29GMT},
abstract = {The cochlear amplifier shows level-dependent function and works optimally at low levels. For this reason, manipulation of stimulus level is a route through which the human cochlear amplifier can be investigated in a noninvasive manner. Distortion product otoacoustic emissions (DPOAEs) evoked as a function of stimulus level provide a tool for exploration of human cochlear amplifier function and, when applied to neonates, for investigation of cochlear maturation. The current experiment generated 2f1{\textminus}f2 DPOAE ipsilateral suppression tuning curves (STCs) at three primary tone levels and five f2 frequencies in a large group of premature and term neonates and adults. The differences between tuning generated with low- and high-level primary tones was measured to provide a gross estimate of the {\textquotedblleft}tuning enhancement effect{\textquotedblright} attributed to the cochlear amplifier. Other features of the DPOAE suppression tuning curves were measured as well. Consistent with previous reports, at 1500 and 6000 Hz, STCs were narrower, with a steeper slope on the low-frequency flank of the tuning curve in premature neonates versus adults. Additionally, only DPOAE STCs from adults and term neonates became markedly broader and more shallow when recorded with high-level primary tones. It has been hypothesized that the excessive narrowness of suppression tuning and the absence of a level effect on DPOAE STCs recorded in premature neonates reflects a subtle immaturity in cochlear amplifier function just prior to term birth.},
url = {http://link.aip.org/link/JASMAN/v110/i3/p1465/s1&Agg=doi},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A2/A2DFE4BA-8C0F-4C3E-A1DB-945C8CA7360D.pdf},
file = {{A2DFE4BA-8C0F-4C3E-A1DB-945C8CA7360D.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A2/A2DFE4BA-8C0F-4C3E-A1DB-945C8CA7360D.pdf:application/pdf;A2DFE4BA-8C0F-4C3E-A1DB-945C8CA7360D.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/A2/A2DFE4BA-8C0F-4C3E-A1DB-945C8CA7360D.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1121/1.1388018}}
}


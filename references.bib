@article{Bian:2011gp,
author = {Bian, Lin and Chen, Shixiong},
title = {{Behaviors of cubic distortion product otoacoustic emissions evoked by amplitude modulated tones.}},
journal = {The Journal of the Acoustical Society of America},
year = {2011},
volume = {129},
number = {2},
pages = {828--839},
month = feb,
affiliation = {Auditory Physiology Laboratory, Department of Speech and Hearing Science, Arizona State University, 3430 Coor Hall, Tempe, Arizona 85287-0102, USA. lin.bian@asu.edu},
doi = {10.1121/1.3531813},
pmid = {21361441},
language = {English},
rating = {0},
date-added = {2015-08-11T23:10:14GMT},
date-modified = {2016-02-15T21:30:33GMT},
abstract = {Distortion product otoacoustic emissions (DPOAEs) were measured using sinusoidal amplitude modulation (AM) tones. When one of the primary stimuli (f(1) or f(2), f(1) < f(2)) was amplitude modulated, a series of changes in the cubic difference tone (CDT) were observed. In the frequency domain, multiple sidebands were present around the CDT and their sizes grew with the modulation depth of the AM stimulus. In the time domain, the CDT showed different modulation patterns between two major signal conditions: the AM tone was used as the f(1) or the f(2). The CDT amplitude followed the AM tone when the f(1) was amplitude modulated. However, when the AM tone acted as the f(2), the CDT showed a more complex modulation pattern with a notch present at the AM tone peak. The relatively linear dependence of CDT on f(1) and the nonlinear relation with f(2) can be explained with a variable gain-control model representing hair cell functions at the DPOAE generation site. It is likely that processing of AM signals at a particular cochlear location depends on whether the hair cells are tuned to the frequency of the carrier. Nonlinear modulation is related to on-frequency carriers and off-frequency carriers are processed relatively linearly.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21361441&retmode=ref&cmd=prlinks},

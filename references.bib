@article{Alexander:1996wq,
author = {Alexander, J E and Bauer, L O and Kuperman, S and Morzorati, S and O'Connor, S J and Rohrbaugh, J and Porjesz, B and Begleiter, H and Polich, J},
title = {{Hemispheric differences for P300 amplitude from an auditory oddball task.}},

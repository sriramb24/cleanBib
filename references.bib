@article{Fisher:2012bx,
author = {Fisher, Jonathan A N and Nin, Fumiaki and Reichenbach, Tobias and Uthaiah, Revathy C and Hudspeth, A J},
title = {{The spatial pattern of cochlear amplification.}},
journal = {Neuron},
year = {2012},
volume = {76},
number = {5},
pages = {989--997},
month = dec,
affiliation = {Howard Hughes Medical Institute and Laboratory of Sensory Neuroscience, The Rockefeller University, 1230 York Avenue, New York, NY~10065, USA.},
doi = {10.1016/j.neuron.2012.09.031},
pmid = {23217746},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-01-09T22:16:16GMT},
date-modified = {2016-02-15T21:30:27GMT},
abstract = {Sensorineural hearing loss, which stems primarily from the failure of mechanosensory hair cells, changes the traveling waves that transmit acoustic signals along the cochlea. However, the connection between cochlear mechanics and the amplificatory function of hair cells remains unclear. Using an optical technique that permits the targeted inactivation of prestin, a protein of outer hair cells that generates forces on the basilar membrane, we demonstrate that these forces interact locally with cochlear traveling waves to achieve enormous mechanical amplification. By perturbing amplification in narrow segments of the basilar membrane, we further show that a cochlear traveling wave accumulates gain as it approaches its peak. Analysis of these results indicates that cochlear amplification produces negative damping that counters the viscous drag impeding traveling waves; targeted photoinactivation locally interrupts this compensation. These results reveal the locus of amplification in cochlear traveling waves and connect the characteristics of normal hearing to molecular forces.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23217746&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/7D/7D16528B-8A63-4F70-B62A-DB1AA20BAACC.pdf},
file = {{7D16528B-8A63-4F70-B62A-DB1AA20BAACC.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/7D/7D16528B-8A63-4F70-B62A-DB1AA20BAACC.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.neuron.2012.09.031}}
}

@article{DurandRivera:2013tfa,
author = {Durand-Rivera, Alfredo and Manzano-Martinez, Ethelvina and Ceballos-Reyes, Guillermo},
title = {{A potential "tuning" role for the outer hair cells in children with language disorders.}},
journal = {Frontiers in Bioscience},
year = {2013},
volume = {5},
pages = {684--696},
affiliation = {Laboratorio de Neuroproteccion, Instituto Nacional de Rehabilitacion, INR- SSa Mexico D, Tlalpan, Mexico. F.alfredo.durand@gmail.com},
pmid = {23277023},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-10-07T16:17:34GMT},
date-modified = {2016-02-15T21:30:28GMT},
abstract = {The cochlear outer hair cells serve a tuning function, and any dysfunction of their electromotile response can be reflected in language disorders. Otoacoustic emissions can be used to determine any dysfunction of these cells. A set of clinical records was established to register the neurological and auditory functioning in 42 children, followed by assessment with the Wechsler Intelligence Scale for Children (WISC), the Initial Language Test (ILT), the Auditory and Phonetic Discrimination Evaluation (APDE), tests for measuring Brainstem Auditory Evoked Potential (BAEP) and Transient Otoacoustic Emissions (TOAE). Subjects were classified into 3 groups in this study: Control (C; n = 20), Syntactic Phonological Disorder (SPD; n = 17), and those with Phonological Disability (PD; n = 5). BAEP studies showed a clear response when all children were stimulated to 20 dB. TOAE responses displayed clear and significant differences with half-octave band reproducibility for both ears, the largest effect being observed in the right ear. The results that were compared using ANOVA tests, showed that cochlear processing affects the brain language function, playing a critical role in the language phonetic process.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23277023&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/D2/D22D9AE6-B2C9-4D85-88CE-EED5DC6B001B.pdf},

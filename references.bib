@article{Ahissar:2007hm,
author = {Ahissar, Merav},
title = {{Dyslexia and the anchoring-deficit hypothesis.}},
journal = {Trends in Cognitive Sciences},
year = {2007},
volume = {11},
number = {11},
pages = {458--465},
affiliation = {Hebrew University, Mount Scopus, Jerusalem, Israel. msmerava@pluto.huji.ac.il},
doi = {10.1016/j.tics.2007.08.015},
pmid = {17983834},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-08-22T11:02:41GMT},
date-modified = {2016-02-15T21:30:25GMT},
abstract = {The 'core deficit' of dyslexia has been characterized by different researchers in terms of either phonological impairments or of difficulties in processing basic visual and auditory stimuli. Recent findings suggest that a single type of impairment in the dynamics of perception, which affects the efficiency of short-term memory, might underlie the broad range of difficulties of dyslexics. Experimental findings show that the general population quickly and automatically tunes around incoming stimuli, 'anchors to them' and performs faster and more accurately when these stimuli are subsequently repeated. Dyslexic individuals fail to benefit from stimulus-specific repetitions. This deficit can account for phonological, working memory, visual and auditory difficulties, in addition to the greater sensitivity of dyslexics to external noise.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=17983834&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/24/244C9B9B-08BE-4966-9D6B-26907E11D5BD.pdf},
file = {{244C9B9B-08BE-4966-9D6B-26907E11D5BD.pdf:/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/24/244C9B9B-08BE-4966-9D6B-26907E11D5BD.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.tics.2007.08.015}}
}


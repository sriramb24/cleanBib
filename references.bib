author = {Tokgoz-Yilmaz, Suna and Kose, Serdal Kenan and Turkyilmaz, Meral Didem and Atay, Gamze},
title = {{The role of the medial olivocochlear system in the complaints of understanding speech in noisy environments by individuals with normal hearing.}},
journal = {Auris, Nasus, Larynx},
year = {2013},
month = may,
affiliation = {Audiology and Speech Pathology Section, Ankara University School of Medicine, Ankara, Turkey. Electronic address: sunatokgoz@yahoo.com.},
doi = {10.1016/j.anl.2013.04.003},
pmid = {23694738},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-05-27T00:52:04GMT},
date-modified = {2016-02-15T21:30:27GMT},
abstract = {OBJECTIVE: The aim of our study is to investigate the relationship between the complaint of speech understanding in noisy environments and the findings of contralateral suppression of transient evoked otoacoustic emissions and speech recognition in noise test methods in individuals with normal hearing. METHODS: Sixty-nine subjects between 18 and 53 years of age with normal hearing participated in the present study. The subjects were assigned to one of two groups, reported difficulty understanding speech in noise or no reported difficulty understanding speech in noise. After hearing and immitancemetric evaluation, contralateral suppression of transient evoked otoacoustic emissions and speech recognition in noise tests were administered to both groups. Suppression was calculated in half-octave frequency bands centered at 1.0, 1.5, 2.0, 3.0 and 4.0kHz. RESULTS: We found out that the speech recognition in noise scores and contralateral suppression values were lower in subjects with the complaint of speech understanding in noise than those who do not have such complaints. CONCLUSIONS: We concluded that the complaint of speech understanding in noise may be related to the medial efferent system dysfunction, so central auditory nervous system.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23694738&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/sbo611/Dropbox/Sriram/Papers3/Library.papers3/Files/20/20E57C66-1B80-452C-87C4-4A6E0EA5EA3A.pdf},

@article{Wilson:2013jk,
author = {Wilson, Wayne J and Arnott, Wendy},
title = {{Using different criteria to diagnose (central) auditory processing disorder: how big a difference does it make?}},
journal = {Journal of Speech, Language, and Hearing Research},
year = {2013},
volume = {56},
number = {1},
pages = {63--70},
month = feb,
affiliation = {University of Queensland, Brisbane, Australia. w.wilson@uq.edu.au},
doi = {10.1044/1092-4388(2012/11-0352)},

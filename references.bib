@article{Anonymous:cSPynZBI,
author = {Ludwig, Alexandra Annemarie and Fuchs, Michael and Kruse, Eberhard and Uhlig, Brigitte and Kotz, Sonja Annette and R{\"u}bsamen, Rudolf},
title = {{Auditory Processing Disorders with and without Central Auditory Discrimination Deficits.}},
journal = {Journal of the Association for Research in Otolaryngology},
year = {2014},
month = mar,
affiliation = {Department of Otorhinolaryngology, Section of Phoniatrics and Audiology, University of Leipzig, Liebigstrasse 10-14, 04103, Leipzig, Germany, alexandra.ludwig@medizin.uni-leipzig.de.},
doi = {10.1007/s10162-014-0450-3},
pmid = {24658855},
